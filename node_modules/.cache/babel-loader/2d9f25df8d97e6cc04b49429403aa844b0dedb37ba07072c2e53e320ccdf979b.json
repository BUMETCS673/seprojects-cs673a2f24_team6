d plans\r\n      plans.value.push({\r\n        name: planName.value,\r\n        details: planDetails.value\r\n      });\r\n\r\n      // Reset input fields after saving the plan\r\n      planName.value = '';\r\n      planDetails.value = '';\r\n    };\r\n\r\n    return {\r\n      planName,\r\n      planDetails,\r\n      plans,\r\n      savePlan\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh2 {\r\n  color: #4CAF50;\r\n}\r\nform {\r\n  margin-bottom: 20px;\r\n}\r\nlabel {\r\n  display: block;\r\n  margin: 10px 0 5px;\r\n}\r\ninput, textarea {\r\n  width: 100%;\r\n  padding: 8px;\r\n  margin-bottom: 10px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\nbutton {\r\n  background-color: #4CAF50;\r\n  color: white;\r\n  padding: 10px 15px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\nbutton:hover {\r\n  background-color: #45a049;\r\n}\r\nul {\r\n  list-style-type: none;\r\n  padding-left: 0;\r\n}\r\n</style>\r\n"],"mappings":";AA0BA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIF,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMG,WAAU,GAAIH,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMI,KAAI,GAAIJ,GAAG,CAAC,EAAE,CAAC;IAErB,MAAMK,QAAO,GAAIA,CAAA,KAAM;MACrB;MACAD,KAAK,CAACE,KAAK,CAACC,IAAI,CAAC;QACfC,IAAI,EAAEN,QAAQ,CAACI,KAAK;QACpBG,OAAO,EAAEN,WAAW,CAACG;MACvB,CAAC,CAAC;;MAEF;MACAJ,QAAQ,CAACI,KAAI,GAAI,EAAE;MACnBH,WAAW,CAACG,KAAI,GAAI,EAAE;IACxB,CAAC;IAED,OAAO;MACLJ,QAAQ;MACRC,WAAW;MACXC,KAAK;MACLC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}